import java.time.LocalDate;

public class PersonaTest {
	public static void main(String[] args) {
		Persona primer = new Persona("Juan","Riquelme",12304563,LocalDate.of(1990,9,10));
		System.out.println(primer);
		System.out.println(Persona.calcularEdad(primer.getFechaNacimiento()));
	}
}
