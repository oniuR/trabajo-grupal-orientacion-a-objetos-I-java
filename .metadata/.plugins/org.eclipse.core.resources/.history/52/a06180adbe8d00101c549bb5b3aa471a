package test;
import modelo.Persona;
import java.time.LocalDate;
import java.time.Month;
public class PersonaTest {
	public static void main(String[] args) {
		Persona primer = new Persona("Juan","Riquelme",12304563,LocalDate.of(1990,Month.MARCH,12));
		System.out.println(primer);
		hoy = LocalDate.now();
		System.out.println(hoy -  primer.getFechaNacimiento());
	}
}
